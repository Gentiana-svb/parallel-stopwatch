		"prettier-plugin-svelte": "^2.8.0",
		"svelte": "^3.52.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.0",
		"typescript": "^4.8.4",
		"vite": "^3.1.8"
	}
}
